  "homepage": "https://github.com/jangxx/node-oscquery#readme",
  "types": "dist/index.d.ts",
  "devDependencies": {
    "@types/node-osc": "^6.0.3",
    "@types/ws": "^8.5.10",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5",
    "ts-node-dev": "^2.0.0"
  },
  "dependencies": {
    "@homebridge/ciao": "^1.3.4",
    "axios": "^1.13.2",
    "bonjour-service": "^1.3.0",
    "node-osc": "^8.1.0",
    "portfinder": "^1.0.38",
    "ws": "^8.19.0"
